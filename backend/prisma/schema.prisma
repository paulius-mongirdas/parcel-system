generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transport {
  id        Int      @id @default(autoincrement())

  type      String
  capacity     Int     @default(0)
  averageSpeed  Int   @default(0)

  @@map("transports")
}